  },
  "attributes": {
    "workspace_name": {
      "type": "string",
      "required": true
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "owner_full_name": {
      "type": "string"
    },
    "owner_email": {
      "type": "email"
    },
    "owner_phone": {
      "type": "string"
    },
    "owner_organization": {
      "type": "string"
    },
    "contact_email": {
      "type": "email"
    },
    "email": {
      "type": "email",
      "required": false
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user"
    },
    "chats": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat.chat",
      "mappedBy": "workspace"
    },
    "documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::document.document",
      "mappedBy": "workspace"
    },
    "knowledge_bases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::knowledge-base.knowledge-base",
      "mappedBy": "workspace"
    },
    "messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::message.message",
      "mappedBy": "workspace"
    },
    "groups": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::group.group",
      "mappedBy": "workspace"
    },
    "workspace_roles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::workspace-role.workspace-role",
      "mappedBy": "workspace"
    },
    "email_verifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::email-verification.email-verification",
      "mappedBy": "workspace"
    },
    "model_icons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::model-icon.model-icon",
      "mappedBy": "workspace"
    },
    "user_files": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-file.user-file",
      "mappedBy": "workspace"
    },
    "workspace_settings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::workspace-setting.workspace-setting",
      "mappedBy": "workspace"
    }
  }
}
