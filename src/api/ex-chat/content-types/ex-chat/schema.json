{
  "kind": "collectionType",
  "collectionName": "ex_chats",
  "info": {
    "singularName": "ex-chat",
    "pluralName": "ex-chats",
    "displayName": "EX Chat",
    "description": "Chat conversations"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "workspaceId": {
      "type": "string",
      "required": true
    },
    "channel": {
      "type": "enumeration",
      "enum": ["widget", "facebook", "instagram", "whatsapp", "line"],
      "required": true
    },
    "widgetSettingId": {
      "type": "string"
    },
    "visitorId": {
      "type": "string",
      "required": true
    },
    "visitorName": {
      "type": "string"
    },
    "visitorAvatar": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": ["open", "closed", "pending"],
      "default": "open",
      "required": true
    },
    "assignee": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "assigneeAgent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::agent.agent"
    },
    "assignedAt": {
      "type": "datetime"
    },
    "assignedBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "assignmentStatus": {
      "type": "enumeration",
      "enum": ["unassigned", "assigned", "auto_assigned"],
      "default": "unassigned"
    },
    "labels": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chat-label.chat-label",
      "inversedBy": "chats"
    },
    "lastMessage": {
      "type": "text"
    },
    "lastMessageAt": {
      "type": "datetime"
    },
    "lastInboundAt": {
      "type": "datetime"
    },
    "lastOutboundAt": {
      "type": "datetime"
    },
    "unreadCount": {
      "type": "integer",
      "default": 0
    },
    "lineSettingId": {
      "type": "string"
    },
    "lineChannelId": {
      "type": "string"
    },
    "lineSettingName": {
      "type": "string"
    },
    "lineUserId": {
      "type": "string"
    },
    "metaSettingId": {
      "type": "string"
    },
    "metadata": {
      "type": "json"
    }
  }
}
